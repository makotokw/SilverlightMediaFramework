								<RepeatButton x:Name="FastForwardElement" Height="28" Margin="-1,0,0,0" Style="{StaticResource FFRepeatButtonStyle}" Width="28" Interval="750" BorderThickness="0" Grid.Column="2"/>
								<Button x:Name="ReplayElement" Height="28" Style="{StaticResource ReplayButtonStyle}" Width="28" Visibility="Collapsed" BorderThickness="0" Grid.Column="3" d:IsHidden="True"/>
								<ToggleButton x:Name="SlowMotionElement" Height="20" Margin="-1,4,0,4" Style="{StaticResource SloMOButtonStyle}" Width="20" Visibility="Collapsed" Grid.Column="4" d:IsHidden="True"/>
								<Button x:Name="ChapterMarkerPreviousElement" Height="20" Margin="-1,0,0,0" Style="{StaticResource ChapterBackButtonStyle}" Width="20" Visibility="Collapsed" Content="pc" Grid.Column="5" d:IsHidden="True"/>
								<Button x:Name="ChapterMarkerNextElement" Height="20" Margin="-1,0,0,0" Style="{StaticResource ChapterForwardButtonStyle}" Width="20" Visibility="Collapsed" Content="nc" Grid.Column="6" d:IsHidden="True"/>
								<p:Scrubber x:Name="PositionElement" Margin="8,2,8,2" VerticalAlignment="Center" Grid.Column="7" AvailableVisibility="Visible"/>
								<Border x:Name="TimeContainer" Margin="0" Grid.Column="8" BorderBrush="#7F434343" BorderThickness="1">
									<Grid Height="28" MaxHeight="28" MinHeight="28" MinWidth="60" UseLayoutRounding="True">
										<Grid>
											<Grid.ColumnDefinitions>
												<ColumnDefinition Width="0.25*"/>
												<ColumnDefinition Width="0.367*"/>
												<ColumnDefinition Width="0.383*"/>
											</Grid.ColumnDefinitions>
											<TextBlock x:Name="CurrentTimeElement" HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="9" Foreground="#FFC1C1C1" Padding="8,0,0,0" TextAlignment="Center"/>
											<TextBlock x:Name="TimeSeparatorElement" HorizontalAlignment="Center" Margin="3,0" VerticalAlignment="Center" Opacity="0.4" Visibility="Collapsed" Grid.Column="1" FontSize="9" Foreground="#FFFDFAFA" Text="|"/>
											<TextBlock x:Name="TotalTimeElement" HorizontalAlignment="Left" VerticalAlignment="Center" Opacity="0.4" Visibility="Collapsed" Grid.Column="2" FontSize="9" Foreground="#FFFDFDFD" Padding="0,0,8,0"/>
										</Grid>
									</Grid>
								</Border>
								<ToggleButton x:Name="GoToLiveElement" Height="28" Margin="-1,0,0,0" Style="{StaticResource GoToLiveButtonStyle}" Width="50" BorderThickness="0" Grid.Column="9"/>
								<p:Bitrate x:Name="BitrateElement" Height="20" Width="20" Visibility="Collapsed" Grid.Column="10"/>
								<Grid x:Name="VolumeExtendWraper" Height="150" Margin="-1,-150,-1,0" VerticalAlignment="Bottom" Width="28" Grid.Column="11">
									<p:VolumeControl x:Name="VolumeElement" HorizontalAlignment="Center" VerticalAlignment="Bottom" Width="28" VolumeLevel=".5"/>
								</Grid>
								<ToggleButton x:Name="FullScreenElement" Style="{StaticResource FullScreenElementStyle}" BorderThickness="0" HorizontalContentAlignment="Stretch" Padding="0" VerticalContentAlignment="Stretch" Grid.Column="12"/>
								<Rectangle x:Name="DividerStoke" Fill="#FF585858" Stroke="Black" StrokeThickness="0" Height="1" VerticalAlignment="Top" Grid.ColumnSpan="13"/>
							</Grid>
						</Grid>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>
	</UserControl.Resources>
    <Grid x:Name="LayoutRoot">
        <p:Player Style="{StaticResource PlayerStyle1}">
